{
  "version": 2,
  "builds": [
    {
      "src": "app_vercel.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "app_vercel.py",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/set-webhook",
      "dest": "app_vercel.py"
    },
    {
      "src": "/get-webhook-info",
      "dest": "app_vercel.py"
    },
    {
      "src": "/delete-webhook",
      "dest": "app_vercel.py"
    },
    {
      "src": "/(.*)",
      "dest": "app_vercel.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}
